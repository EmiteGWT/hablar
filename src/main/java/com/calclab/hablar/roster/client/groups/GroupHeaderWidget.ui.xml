<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.calclab.hablar.icons.client.IconsBundle" field="res" />
	<g:HTMLPanel
		addStyleNames="hablar-GroupHeaderWidget hablar-lightBackground hablar-bottomDarkBordered">
		<!-- g:Image resource="{res.missingIcon}" ui:field="toggle"
			addStyleNames="toggleButton" / -->
		<g:Image resource="{res.menuIcon}" ui:field="menu"
			addStyleNames="menu" />
		<g:Label ui:field="name" addStyleNames="name">name</g:Label>
	</g:HTMLPanel>
</ui:UiBinder>